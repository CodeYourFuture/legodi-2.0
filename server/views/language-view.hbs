<div class="mt-5 weegie-question">
    {{#each data}}

    <h3 class="text-center">Language ID:</h3>
    <h4 class="text-center">{{this.language_id}}</h4>

    <h3 class="text-center mt-4">Original Name:</h3>
    <h4 class="text-center">{{this.original_name}}</h4>

    <h3 class="text-center mt-4">Short Name:</h3>
    <h4 class="text-center">{{this.short_name}}</h4>

    <h3 class="text-center mt-4">Long Name:</h3>
    <h4 class="text-center">{{this.long_name}}</h4>

    {{/each}}
</div>